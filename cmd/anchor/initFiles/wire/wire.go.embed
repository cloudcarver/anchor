//go:build wireinject
// +build wireinject

package wire

import (
	anchor_wire "github.com/cloudcarver/anchor/wire"
	"github.com/cloudcarver/test-app/internal/asynctask"
	"github.com/cloudcarver/test-app/internal/config"
	"github.com/cloudcarver/test-app/internal/handler"
	"github.com/cloudcarver/test-app/internal/zcore/initapp"
	"github.com/cloudcarver/test-app/internal/zcore/model"
	"github.com/cloudcarver/test-app/internal/zgen/taskgen"
	"github.com/google/wire"
)

func InitApp() (*initapp.App, error) {
	wire.Build(
		anchor_wire.InitializeApplication,
		initapp.NewApp,
		handler.NewHandler,
		handler.NewValidator,
		taskgen.NewTaskHandler,
		asynctask.NewExecutor,
		model.NewModel,
		config.NewConfig,
	)
	return nil, nil
}
